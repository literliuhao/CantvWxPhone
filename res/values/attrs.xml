    <!-- 图片布局,由于图片在选中时需要显示文本 -->
    <declare-styleable name="ImageRelativeLayout">
        <attr name="scaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <!-- Sets a drawable as the content of this ImageView. -->
        <attr name="src" format="reference|color" />

        <!-- textview -->
        <attr name="txt_align" format="reference|integer" />
        <attr name="textSize" format="dimension" />
        <attr name="text" format="string" localization="suggested" />
        <attr name="textColor" format="reference|color" />
        <attr name="txtPaddingLeft" format="dimension" />
        <!-- Sets the padding, in pixels, of the top edge; see {@link android.R.attr#padding}. -->
        <attr name="txtPaddingTop" format="dimension" />
        <!-- Sets the padding, in pixels, of the right edge; see {@link android.R.attr#padding}. -->
        <attr name="txtPaddingRight" format="dimension" />
        <!-- Sets the padding, in pixels, of the bottom edge; see {@link android.R.attr#padding}. -->
        <attr name="txtPaddingBottom" format="dimension" />
        <attr name="alwaysShowTxt" format="boolean" />
    </declare-styleable>

